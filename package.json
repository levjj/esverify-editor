{
  "name": "esverify-web",
  "version": "0.0.3",
  "description": "esverify web editor",
  "author": "Christopher Schuster <cs@livoris.net>",
  "license": "MIT",
  "scripts": {
    "build": "lessc src/style.less build/style.css && jspm bundle-sfx app.js - child_process build/app.js --globals \"{child_process:'{}'}\" --format global --skip-source-maps && cat src/header.html src/index.html src/footer.html > build/index.html && cat src/header.html src/try.html src/footer.html > build/try.html && cp src/logo.png build",
    "serve": "node serve.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/levjj/esverify-web.git"
  },
  "dependencies": {
    "express": "^4.15.3"
  },
  "devDependencies": {
    "jspm": "^0.16.53",
    "less": "^2.7.2"
  },
  "jspm": {
    "directories": {
      "baseURL": "src"
    },
    "dependencies": {
      "brace": "npm:brace@^0.10.0",
      "esverify": "npm:esverify@^0.1.6",
      "jquery": "npm:jquery@^3.2.1",
      "spectre.css": "npm:spectre.css@^0.2.14"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
